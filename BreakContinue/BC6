package breaks;

public class BreakContinue6 {
	
	public static void main(String[] args) {
		
		for (char ch = 'A'; ch <= 'Z'; ch++) {
            if (ch == 'A' || ch == 'E' || ch == 'I' || ch == 'O' || ch == 'U') {
                continue;   // skip vowels
            }
            System.out.print(ch + " ");
        }
		
		
	}

}
