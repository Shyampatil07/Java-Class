package loops;

public class Loops22 {
	
	public static void main(String[] args) {
		//2.Print the table of 15 by using 3 loops
		
//		for(int i=1;i<=10;i++) {
//			System.out.println(i*15);
//		}
//		
		
//		int i=1;
//		while(i<=10) {
//			System.out.println(i*15);
//			i++;
//		}
//		
		
		int i=1;
		do {
			System.out.println(i*15);
			i++;
		}while(i<=10);
		
	}

}
