package loops;

public class Loops1 {
	
	public static void main(String[] args) {
		
//	//1	//Print numbers from 1 to 10 using a loop.
//		for(int i=1;i<=10;i++) {
//			System.out.println(i);
//		}
//		
		
		//2 Print numbers from 10 to 1.
		
		for (int i=10;1<=i;i--) {
			System.out.println(i);
		}
		
		
	}

}
