	   Write a program to remove duplicate elements from a 1D array.
	
	   int [] arr= {4,967,9,74,6,75,97,98,9};
	  
	   for (int i = 0; i < arr.length; i++) { 
		   boolean ischeck=false;
           for (int j = i + 1; j < arr.length; j++) {       	   
        	   
        	   if(arr[i]==arr[j]) {
        		   
        		   ischeck=true;
        		   break;
        	   }        
           }
           if(!ischeck) {
 			  System.out.print(arr[i] + " ");
 		}
           }
	
