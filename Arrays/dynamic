//Write a program to declare and initialize a 1D array of integers.
	
	
	int [] a= {1,2,3,4,5};//static
	for(int i=0; i < a.length;i++) {
	System.out.println(a[i]);
}
	int []b=new int [5];
	b[0]=1;//dynamic
	b[1]=2;
	b[2]=3;
	b[3]=4;
	b[4]=5;
	for(int i=0; i < a.length;i++) {
		System.out.println(a[i]);
	}
	
