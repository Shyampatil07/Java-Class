   //Write a program to find the second largest element in a 1D array.
	
	    int [] arr= {4,967,9,74,6,75,97,98,9};
	  int max=arr[0];
	  int secmax=arr[0];
	  for (int i = 0; i < arr.length; i++) {
		  if(arr[i]>max) {
			  secmax = max;
			  max= arr[i];
		  }
		  else if(arr[i]>secmax && arr[i] != secmax) {
			  secmax=arr[i];
		  }
	  }
	  System.out.println(secmax);
	   
