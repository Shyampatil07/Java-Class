	//Write a program to check whether a given number exists in the array or not.

	int [] arr= {1,7,9,8,876,897};
	int num=87;
	boolean ischeck=false;
	
	for(int i=0;i<arr.length;i++) {
      if(arr[i]==num) {
    	
          ischeck=true;
      }
     
      }
	if(ischeck==true) {
		  System.out.println("yes");
	}else {
	  System.out.println("no");
	}
	
	
