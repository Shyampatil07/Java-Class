package multithreading;

public class Multi extends Thread {
	
		public void m1() {
			for(int i=454;i<542;i++) {
				System.out.println(i);
				try {
					Thread.sleep(1000);
				} catch (Exception e) {
					e.printStackTrace();
				}
			}
	}
		
	@Override
	public void run() {
		System.out.println("Shyam");
		for(int i=0;i<111;i++) {
			System.out.println(i);
			try {
				Thread.sleep(1000);
			} catch (Exception e) {
				e.printStackTrace();
			}
			
			
		
